generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  name     String?
  isValidEmail Int
  otpCode String
  password String
}
model Category {
  id       Int     @id @default(autoincrement())
  name     String @unique
}
